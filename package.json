{
  "name": "rosen-scanner",
  "version": "0.1.0",
  "description": "A Blockchain scanner. currently supports ergo and cardano",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "rosen-scanner/node_modules/.bin/eslint --fix .",
    "test": "rosen-scanner/node_modules/.bin/rimraf sqlite; tsc --noEmit; npm run lint; jest",
    "coverage": "rosen-scanner/node_modules/.bin/c8 npm run test",
    "build": "rosen-scanner/node_modules/.bin/rimraf dist && tsc"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "ergo-lib-wasm-nodejs": "^0.18.0",
    "sqlite3": "^5.0.9",
    "typeorm": "^0.3.7"
  },
  "type": "module",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.26.0",
    "c8": "^7.11.3",
    "eslint": "^8.16.0",
    "jest": "^28.1.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
